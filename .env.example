VIBE_CLI_USER_EMAIL=admin@example.com

NEXT_PUBLIC_DEBUG_PRODUCTION="false"
ENABLE_ANALYTICS="false"

# Database Configuration
# Local development database
DATABASE_URL="postgres://postgres:postgres@localhost:5432/postgres?schema=public"
# Example production database URL (replace with your actual database URL)
# DATABASE_URL="postgres://username:password@host:port/database?sslmode=require"

# SSL Configuration for Supabase
# When using Supabase, you may need to disable SSL certificate verification
# This is only recommended for development environments
# NODE_TLS_REJECT_UNAUTHORIZED=0

# Application Configuration
NEXT_PUBLIC_APP_URL="http://localhost:3000"
NEXT_PUBLIC_TEST_SERVER_URL="http://localhost:4000"
NODE_ENV="development" # Options: development, production, test
JWT_SECRET_KEY=your-secret-key-here-minimum-32-characters
CRON_SECRET=your-secret-key-here-minimum-32-characters


GOOGLE_SERVICE_ACCOUNT_EMAIL="your-google-service-account-email@gmail.com"
GOOGLE_PRIVATE_KEY="-----BEGIN PRIVATE KEY-----\n...\n-----END PRIVATE KEY-----\n"
GOOGLE_CALENDAR_ID_DEFAULT="your-default-google-calendar-id"

# AI Configuration
