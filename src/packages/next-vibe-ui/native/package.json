{
  "name": "next-vibe-ui",
  "version": "1.0.0",
  "private": true,
  "main": "ui/index.ts",
  "exports": {
    "./ui/*": {
      "react-native": "./ui/*.tsx",
      "default": "./ui/*.tsx"
    },
    "./hooks/*": {
      "react-native": "./hooks/*.tsx",
      "default": "./hooks/*.tsx"
    },
    "./lib/*": {
      "react-native": "./lib/*.ts",
      "default": "./lib/*.ts"
    }
  },
  "peerDependencies": {
    "react": "*",
    "react-native": "*",
    "react-native-css": "*"
  }
}